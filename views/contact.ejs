<div class="col-12 mt-5 container-xl formwala   w-70">
    <div class="contract-inner-wrapperz">
      <div>
        <h3 class="contract-tittle">Contract us here  </h3>
        <form action="" class="d-flex flex-column gap-10 formwala">
          <div class="mb-3 w-100">
         
            <input
              type="text"
              class="form-control input-wrapper"
              id="exampleFormControlInput1"
              placeholder="Name"
            />
          </div>
          <div class="mb-3 w-100">
         
         <input
           type="email"
           class="form-control input-wrapper "
           id="exampleFormControlInput1"
           placeholder="Email"
         />
